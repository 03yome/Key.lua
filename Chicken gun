local Library = loadstring(Game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wizard"))()
local PhantomForcesWindow = Library:NewWindow("Chicken gun")
local KillingCheats = PhantomForcesWindow:NewSection("guns")
KillingCheats:CreateButton("Inf sniper", function()
local args = {
	999
}
game:GetService("Players").LocalPlayer.Character:WaitForChild("Sniper"):WaitForChild("Events"):WaitForChild("SpareAmmoEvent"):FireServer(unpack(args))
print("HI")
end)
KillingCheats:CreateButton("inf mini gun", function()
local args = {
