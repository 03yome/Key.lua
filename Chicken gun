local KillingCheats = PhantomForcesWindow:NewSection("Badge")
KillingCheats:CreateButton("PitchFork", function()

local Players = game:GetService("Players")
local player = Players.LocalPlayer

local badgeFolder = player:WaitForChild("BadgeFolder")

local boolValue = badgeFolder:FindFirstChild("1426512294234884")
if boolValue and boolValue:IsA("BoolValue") then
    boolValue.Value = true  -- Bật (tick)
    -- boolValue.Value = false -- Tắt (bỏ tick)
end

print("HI")
end)
KillingCheats:CreateButton("idk", function()
-- LocalScript (StarterPlayerScripts hoặc chạy ở client)
local Players = game:GetService("Players")
local player = Players.LocalPlayer

-- Thư mục BadgeFolder trong player
local badgeFolder = player:WaitForChild("BadgeFolder")

-- Ví dụ đổi giá trị BoolValue có tên cụ thể
local boolValue = badgeFolder:FindFirstChild("2703432552443860")
if boolValue and boolValue:IsA("BoolValue") then
    boolValue.Value = true  -- Bật (tick)
    -- boolValue.Value = false -- Tắt (bỏ tick)
end
print("HI")
end)
